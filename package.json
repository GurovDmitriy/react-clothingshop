{
  "name": "react-clothing-store",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:output": "npx serve@latest out",
    "codegen": "graphql-codegen",
    "lint": "npm run lint:next && npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:next": "next lint",
    "lint:eslint": "eslint --fix --ext \".jsx,.js,.ts,.tsx\" --ignore-path .eslintignore .",
    "lint:prettier": "prettier --write \"**/*.{jsx,js,ts,tsx,css,less,scss,html,md}\" --ignore-path .prettierignore",
    "lint:stylelint": "stylelint --fix \"**/*.{css,less,scss}\" --ignore-path .stylelintignore",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll"
  },
  "dependencies": {
    "@urql/next": "^1.1.1",
    "antd": "^5.16.1",
    "clsx": "^2.1.0",
    "firebase": "^10.6.0",
    "firebase-admin": "^12.0.0",
    "next": "^14.1.4",
    "next-auth": "^4.24.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server-only": "^0.0.1",
    "sharp": "^0.33.3",
    "swr": "^2.2.5",
    "urql": "^4.0.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "eslint": "^8",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "graphql": "^16.8.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.13",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "sass": "^1.74.1",
    "stylelint": "^16.3.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-rational-order-fix": "^0.1.9",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
  },
  "lint-staged": {
    "**/*.{jsx,js,ts,tsx}": "eslint --fix",
    "**/*.{css,less,scss}": "stylelint --fix",
    "**/*.{js,jsx,ts,tsx,json,css,scss,html,md}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "20.12.1"
  }
}
