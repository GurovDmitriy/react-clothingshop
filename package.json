{
  "name": "react-clothing-store",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "codegen": "graphql-codegen",
    "lint": "npm run lint:next && npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:next": "next lint",
    "lint:eslint": "eslint --fix --ext \".jsx,.js,.ts,.tsx\" --ignore-path .eslintignore .",
    "lint:prettier": "prettier --write \"**/*.{jsx,js,ts,tsx,css,less,scss,html,md}\" --ignore-path .prettierignore",
    "lint:stylelint": "stylelint --fix \"**/*.{css,less,scss}\" --ignore-path .stylelintignore",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/interaction": "^6.1.10",
    "@fullcalendar/react": "^6.1.10",
    "@reduxjs/toolkit": "^2.0.1",
    "@urql/next": "^1.1.0",
    "antd": "^5.11.5",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "firebase": "^10.7.1",
    "immer": "^10.0.3",
    "next": "^14.0.3",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.5",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "server-only": "^0.0.1",
    "sharp": "^0.33.0",
    "swr": "^2.2.4",
    "urql": "^4.0.6",
    "use-immer": "^0.9.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-big-calendar": "^1.8.5",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.7",
    "eslint": "^8",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "graphql": "^16.8.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-rational-order-fix": "^0.1.9",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.3.2"
  },
  "lint-staged": {
    "**/*.{jsx,js,ts,tsx}": "eslint --fix",
    "**/*.{css,less,scss}": "stylelint --fix",
    "**/*.{js,jsx,ts,tsx,json,css,scss,html,md}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "20.10.0"
  }
}
