{
  "name": "learn-react-clothingshop",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "classnames": "^2.3.2",
    "firebase": "^9.14.0",
    "postcss-normalize": "^10.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "sass": "^1.56.1",
    "web-vitals": "^3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "prepare": "husky install",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:eslint": "eslint --fix --ext \".jsx,.js\" --ignore-path .eslintignore .",
    "lint:prettier": "prettier --write \"**/*.{jsx,js,css,less,scss,html,md}\" --ignore-path .prettierignore",
    "lint:stylelint": "stylelint --fix \"**/*.{css,less,scss,html}\" --ignore-path .stylelintignore"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "src/**/*.{jsx,js}": "eslint --fix",
    "src/**/*.{css,less,scss,html}": "stylelint --fix",
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@redux-devtools/core": "^3.13.1",
    "eslint": "^8.28.0",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "2.8.0",
    "prettier-plugin-organize-imports": "^3.2.0",
    "stylelint": "^14.15.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-rational-order-fix": "^0.1.9",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^5.0.0"
  },
  "packageManager": "yarn@3.3.0"
}
